{
  "name": "example-app",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test": "ng test --watch=false --browsers=Chrome",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "build": "npm run build:libs && ng build",
    "lint:design": "ng lint @mafo5/ng-design",
    "test:design": "ng test @mafo5/ng-design --watch=false --browsers=Chrome",
    "test:design:ci": "ng test @mafo5/ng-design --watch=false --browsers=ChromeHeadlessCustom",
    "build:design": "ng build @mafo5/ng-design --prod",
    "lint:ref": "ng lint @mafo5/ng-ref",
    "test:ref": "ng test @mafo5/ng-ref --watch=false --browsers=Chrome",
    "test:ref:ci": "ng test @mafo5/ng-ref --watch=false --browsers=ChromeHeadlessCustom",
    "build:ref": "ng build @mafo5/ng-ref --prod",
    "build:libs": "npm run build:ref && npm run build:design",
    "dev:ref": "ng test @mafo5/ng-ref",
    "e2e": "ng e2e",
    "deploy": "npm run build:libs && ng deploy --base-href=/exampleAppAngular/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "hamjest": "^3.7.1",
    "hamjest-sinon": "^1.0.5",
    "rxjs": "^6.6.3",
    "sinon": "^9.2.2",
    "tslib": "^2.0.0",
    "webflow-api": "^0.5.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.9",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "puppeteer": "^3.3.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
